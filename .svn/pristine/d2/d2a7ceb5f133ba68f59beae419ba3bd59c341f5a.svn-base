/*add By fanjibing new Menu**/
#navMenu a {
	text-decoration: none;
}
#navMenu a:hover {
	text-decoration: none;
}
#navMenu {
	top: 0px; height: 52px; left: 200px; float: left; position: absolute; z-index: 8999;
}
#navMenu .root {
	height: 52px; line-height: 52px; padding-top: 0px;
}
.root > li {
	background: url("../img/fenge.png") no-repeat right; margin: 0px; padding: 0px; width: 90px; height: 52px; float: left; position: relative;
}
.root > li:last-child {
	background: none;
}
.root > li:hover > a {
	background: url("../img/tops.png") no-repeat center; color: rgb(71, 71, 71);
}
.root > li > a {
	left: -2px; width: 100%; text-align: center; color: rgb(255, 255, 255); overflow: hidden; font-size: 16px; display: block; white-space: nowrap; position: absolute; z-index: 10;
}
.dropdown-menu {
	background: rgb(255, 255, 255); border: 1px solid rgb(218, 218, 218); border-image: none; display: none; position: absolute;
}
.dropdown-menu > li {
	background: none; margin: 0px; padding: 0px; width: 160px; height: 31px; line-height: 31px; position: relative;
}
.dropdown-menu > li > a {
	color: rgb(0, 0, 0); padding-left: 25px; font-size: 14px; font-weight: normal; border-bottom-color: rgb(218, 218, 218); border-bottom-width: 1px; border-bottom-style: dotted; display: block;
}
.dropdown-menu > li > a:hover {
	background: rgb(237, 237, 237); color: rgb(19, 101, 171);
}
.wodry {
	width: 100%; color: rgb(255, 133, 0); display: inline-block;
}
