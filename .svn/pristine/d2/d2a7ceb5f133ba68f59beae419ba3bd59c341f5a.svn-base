/**日期选择器**/
.dateSelect,
.dateSelect1 {
    margin-left: 10px;
}
.dateSelect .dateBtn,
.dateSelect1 .dateBtn{
    margin: 0px 0px 0px 0px;
    cursor:pointer;
}
.dateBtn.dateBtnUp,
.dateBtn.dateBtnDown {
	position: relative;
	width: 14px;
	height: 7px;
}

.dateSelect .dateBtn.dateBtnUp,
.dateSelect1 .dateBtn.dateBtnUp {
	top: -8px;
	left: 7px;
}

.dateSelect .dateBtnDown,
.dateSelect1 .dateBtnDown {
	top: 3px;
	left: -7px;
}

