/*checkboxCircle*/
div.checkboxCircle {
    background: #DDD none repeat scroll 0% 0%;
    border-radius: 100%;
    position: relative;
    box-shadow: 0px 1px 3px rgba(0, 0, 0, 0.5);
    float: right;
    margin-right: 10px;
    width: 14px;
    height: 14px;
    margin-top: 2px;
}

div.checkboxCircle input {
    display: none;
}

div.checkboxCircle label {
    display: block;
    width: 10px;
    height: 10px;
    border-radius: 100px;
    transition: all 0.5s ease 0s;
    cursor: pointer;
    position: absolute;
    z-index: 1;
    background: #FFF none repeat scroll 0% 0%;
    box-shadow: 0px 1px 3px rgba(0, 0, 0, 0.5) inset;
    top: 2px;
    left: 2px;
}

div.checkboxCircle input[type=checkbox]:checked + label {
    background: rgb(52,152,219);
}

