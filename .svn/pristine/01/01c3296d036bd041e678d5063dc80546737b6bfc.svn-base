@charset "utf-8";
@themecolor:#2591F1;
@titlecolor:#3E85BB;
@bodercolor:rgba(125,125,125,1);
@lineheight:30px;
@tablehearcolor:rgba(230, 230, 230, 1);
@paneltitlefontsize:14px;
@paneltypefontsize:13px;
body{
	position: absolute;
	left:0px;
	top:0px;
	width: 100%;
	height: 100%;
	overflow-x: hidden;
	overflow-y: hidden;
	font-family: "Microsoft YaHei";
}
ul{
	list-style: none;
	padding: 0px;
	li{
		height: 30px;
		line-height: 30px;
	}
}
span{
	text-align: center;
}
.returnIndex{
	font-size: 18px;
	font-weight: bold;
	color:#FFFFFF;
	cursor: pointer;
}
.mainMenu{
	font-size: 18px;
	font-weight: bold;
}
.banner{
    height: 60px;
    position: absolute;
    width: 100%;
    color: #fff;
    background-color: @themecolor;
}
.logo{
    float: left;
    height: 60px;
    margin: 0px 0px 0px 20px;
    line-height: 60px;
}
.logo img{
    height: 100%;
}
.logo .title{
    margin-left: 10px;
    font-size: 28px;
    vertical-align: top;
    letter-spacing: 5px;
}
#login{
    position: absolute;
    right: 10px;
    top:10px;
    font-size: 14px;
}
#login a{
    margin-left: 10px;
    cursor: pointer;
}
/*页面导航按钮*/
.navigation{
    position: absolute;
    right: 150px;
    bottom: 5px;
}
.navigation button{
    padding: 5px 10px;
    font-size: 16px;
    font-family: "Microsoft Yahei";
    margin: 0px 10px 0px 10px;
    border: 0px;
    color:#fff;
    background: none;
    cursor: pointer;
    outline:none;
}
.navigation button:hover,.navigation button.active{
    border-bottom:solid #f7921c 4px;
    outline:none;
}
#map_div{
    height: 100%;
}
#map{
    position: absolute;
    left:0px;
    top: 0px;
    bottom: 0px;
    right: 0px;
}
.menu{
    height: 100%;
    border-right: 1px solid #d0d1d1;
    background: @themecolor;
    z-index: 5;
    box-shadow: 3px 0px 15px 0px #a0a1a3;
    padding: 20px;
    color:white;
    .space{
    	height: 20px;
    }
    .space5{
    	height: 5px!important;
    }
    .title{
    	height: 30px;
    	border-bottom: 1px solid white;
    	padding-bottom: 10px;
    	span{
    		font-size: 18px;
    	}
    	img{
    		width: 30px;
    		height: 30px;
    	}
    }
    .con{
    	height: 30px;
    	display: flex;
	span{
		display: block;
		flex:1;
		line-height: 30px;
		cursor: pointer;
		margin-right: 2px;
	}
	span.active{
		background-color: orange;
	}
	p{
		line-height: 30px;
		display: block;
		font-weight: bold;
		width: 60px;
	}
    }
    #alarm{
    	bottom: 0px;
    	height: 40px;
    	background-color: #9DC6FF;
    	position: fixed;
    	left: 0px;
    	width: 299px;
    	display: flex;
    	.half{
    		line-height: 40px;
    		text-align: center;
    		flex:1;
    		display:flex;
    		div{
    			padding: 5px;
    		}
    	}
    }
    #ljyb_model{
    	div{
		height: 50px;
		padding: 5px;
		line-height: 40px;
    	}
    }
}
.menu_normal{
    width: 300px;
}
.menu_minimize{
    left: 56px;
    width: 0px;
}
#menu_bd{
    position: relative;
    height: 100%;
    z-index: 10;
    overflow: auto;
    background: rgba(225, 225, 235,1);
}
#menu_bd .title1{
    font-size: 14px;
    color: #006699;
    padding: 5px 0px 0px 10px;
}
#menu_bd .title2{
    font-size: 12px;
    color: #a06c94;
    padding: 5px 0px 5px 20px;
}
#menu_bd .btn_line{
    padding: 0px 0px 5px 35px;
}
#menu_bd .btn_line1, #menu_bd .btn_line2{
    padding: 0px 0px 5px 70px;
}
#menu_bd .btn_line3{
    padding: 0px 0px 5px 35px;
}
#menu_bd .btn_line4{
    padding: 0px 0px 5px 70px;
}
#menu_bd .btn_line5{
    padding: 0px 0px 5px 35px;
}
#menu_bd button{
    border: solid 1px rgb(150,160,200);
    padding: 5px 0px;
    border-radius: 5px;
    cursor:pointer;
    margin: 0px 0px 0px 10px;
    font-size: 12px;
    background: rgba(225, 225,235,1);
    box-shadow: 1px 1px 1px rgb(180,190,200);
}
